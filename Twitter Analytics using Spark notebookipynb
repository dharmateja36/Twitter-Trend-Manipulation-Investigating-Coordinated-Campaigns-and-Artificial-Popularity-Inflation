    "    .agg(countDistinct(when(col(\"sourcehashtag\") == \"HappyBirthdayNTR\", col(\"username\"))).alias(\"HappyBirthdayNTR_count\"),\n",
    "         countDistinct(when(col(\"sourcehashtag\") == \"NTRBdayFestBegins\", col(\"username\"))).alias(\"NTRBdayFestBegins_count\")) \\\n",

   "source": [
    "# Average retweets per account\n",
    "avg_retweets_per_account = twitterdataanalysis \\\n",
    "    .groupBy(\"sourcehashtag\", \"user\") \\\n",
    "    .agg(avg(\"retweetCount\").alias(\"avg_retweets_per_user\")) \\\n",
    "    .groupBy(\"sourcehashtag\") \\\n",
    "    .agg(avg(\"avg_retweets_per_user\").alias(\"value\")) \\\n",
    "    .withColumn(\"metric\", lit(\"avg_retweets_per_account\")) \\\n",
    "    .select(\"sourcehashtag\", \"metric\", \"value\")"
   ]
  },

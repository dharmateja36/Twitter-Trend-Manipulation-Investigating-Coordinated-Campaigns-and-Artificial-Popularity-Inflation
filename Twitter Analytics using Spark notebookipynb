    "# total tweets for each hashtag\n",
    "total_tweets_df = twitterdataanalysis.groupBy(\"sourcehashtag\") \\\n",
    "    .agg(count(\"*\").alias(\"value\")) \\\n",
    "    .withColumn(\"metric\", lit(\"total_tweets\")) \\\n",
    "    .select(\"sourcehashtag\", \"metric\", \"value\")"
